  .zc_block .btn a:visited,.zc_block .btn a:hover{color: #fff;}
  .zc_block .list{
    border-top:1px solid #c5cad9;
  }
  .zc_block .list p{
    position: relative;
    float: left;
    width: 120px;
    height: 26px;
    line-height: 26px;
    text-align: center;
    overflow: hidden;
  }
</style>


